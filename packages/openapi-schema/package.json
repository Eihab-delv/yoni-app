{
  "name": "@repo/openapi-schema",
  "description": "Auto-generated TS types from openAPI specifications for frontend use",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "tsup && tsc --emitDeclarationOnly && tsc-alias --verbose",
    "gen:schema": "./gen-schema.sh"
  },
  "license": "MIT",
  "dependencies": {
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.8.0",
    "typescript": "^5.8.3",
    "@codecompose/typescript-config": "^1.2.0",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.3.6",
    "typescript-eslint": "^8.21.0"
  }
}
